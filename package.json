{
  "name": "louvre-museum",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://andreizayaz.github.io/louvre-museum/",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.9.7",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.2",
    "@mui/x-date-pickers": "^5.0.0-beta.4",
    "@reduxjs/toolkit": "^1.8.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "animate.css": "^4.1.1",
    "concurrently": "^7.3.0",
    "husky": "^8.0.1",
    "i18next": "^21.8.8",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "jest-image-snapshot": "^5.1.0",
    "jest-transform-css": "^4.0.1",
    "libphonenumber-js": "^1.10.12",
    "material-ui-popup-state": "^3.1.1",
    "node-sass": "^7.0.1",
    "puppeteer": "^14.4.0",
    "react": "^18.1.0",
    "react-compare-slider": "^2.2.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.34.0",
    "react-i18next": "^11.17.0",
    "react-intersection-observer": "^9.3.5",
    "react-player": "^2.10.1",
    "react-redux": "^8.0.2",
    "react-scripts": "5.0.1",
    "react-select": "^5.4.0",
    "react-test-renderer": "^18.1.0",
    "redux-persist": "^6.0.0",
    "screenfull": "^6.0.2",
    "swiper": "^8.2.4",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "stylelint": "npx stylelint **/*.scss",
    "stylelint-fix": "npx stylelint **/*.scss --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format-check": "prettier --check ./**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc",
    "format-fix": "prettier --write ./**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc",
    "fix": "concurrently \"yarn stylelint-fix\" \"yarn lint-fix\" \"yarn format-fix\"",
    "prepare": "husky install"
  },
  "jest": {
    "moduleNameMapper": {
      "swiper/react": "swiper/react/swiper-react.js",
      "swiper/css": "swiper/swiper.min.css"
    },
    "transform": {
      "^.+\\.css$": "jest-transform-css"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(swiper|ssr-window|dom7)/)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest-image-snapshot": "^4.3.1",
    "@types/puppeteer": "^5.4.6",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-helmet": "^6.1.5",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "flag-icons": "^6.4.5",
    "gh-pages": "^4.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^4.0.0"
  }
}
